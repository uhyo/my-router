{
  "compilerOptions":{
    "lib": ["dom", "es2015"],
    "alwaysStrict": true,
    "declaration": true,
    "module":"commonjs",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "removeComments": true,
    "target":"es5",
    "outDir":"dist"
  },
  "files":[
    "lib/pathutil.ts",
    "lib/state.ts",
    "lib/index.ts",
    "index.ts"
  ]
}
